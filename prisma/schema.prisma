// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model KeyValueItem {
  key   String @id
  value Json
}

model Quote {
  id        Int     @id @default(autoincrement())
  guildId   String
  userId    String
  messageId String? @unique
  message   Json
}

model Mute {
  id       Int      @id @default(autoincrement())
  guildId  String
  memberId String
  endsAt   DateTime
}
